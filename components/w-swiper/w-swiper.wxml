<swiper class="swiper"
    circular
    autoplay
    interval='3000'
    duration='300'
    indicator-dots
    indicator-active-color='#791BAC'>
    <block wx:for="{{list}}" wx:key="index">
    <swiper-item class="swiper-item">
    <image src="{{baseURL}}{{item.url}}" mode="widthFix"></image>
    <!-- <view class="urltext">{{item.copyright}}</view> -->
    </swiper-item>
    </block>
  </swiper>
